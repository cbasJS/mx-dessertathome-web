{
  "name": "mx-dessertathome-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "build:app": "gatsby clean && gatsby build",
    "serve": "gatsby serve -p ${PORT:-9000}",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "coverage": "jest --coverage",
    "deploy": "yarn deploy:infra; yarn deploy:app:aws",
    "deploy:infra": "sls deploy --stage ${STAGE:-dev}",
    "deploy:app:aws": "aws s3 rm s3://$AWS_S3_BUCKET --recursive && aws s3 cp ./public s3://$AWS_S3_BUCKET --recursive && aws cloudfront create-invalidation --distribution-id $AWS_CLOUDFRONT_DISTRIBUTION_ID --paths '/'"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "apollo-boost": "^0.4.7",
    "cross-fetch": "^3.0.4",
    "gatsby": "^2.21.11",
    "gatsby-plugin-apollo": "^3.0.1",
    "gatsby-plugin-manifest": "^2.4.1",
    "gatsby-plugin-offline": "^3.2.0",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-plugin-sitemap": "^2.4.1",
    "gatsby-plugin-styled-components": "^3.3.0",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-source-prismic-graphql": "^3.6.2",
    "gatsby-source-shopify": "^3.2.0",
    "gatsby-transformer-sharp": "^2.5.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-responsive": "^8.0.3",
    "styled-components": "^5.1.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@apollo/client": "^3.0.0-beta.44",
    "@babel/preset-typescript": "^7.9.0",
    "@silvermine/serverless-plugin-cloudfront-lambda-edge": "^2.1.1",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/query-string": "^6.3.0",
    "@types/react-dom": "^16.9.7",
    "@types/react-helmet": "^5.0.15",
    "@types/react-responsive": "^8.0.2",
    "@types/styled-components": "^5.1.0",
    "babel-jest": "^26.0.1",
    "babel-preset-gatsby": "^0.4.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "jest": "^26.0.1",
    "serverless": "^1.70.0"
  }
}
